shared_writable_dirs: 
  - /manifest/cache
  - /manifest/logs
  - /manifest/tmp
  - /workspace
  
url_rewrites:
   - ^hyperspaces/appcast/?$									    /shimmer/?appcast&appName=Hyperspaces&appVariant=Stable									[QSA,L]
   - ^hyperspaces/notes/?$										    /shimmer/?notes&appName=Hyperspaces&appVariant=Stable									[QSA,L]
   - ^hyperspaces/notes/([^/]+)/?$								/shimmer/?notes&appName=Hyperspaces&appVariant=Stable&appVersion=$1			            [QSA,L]
   - ^hyperspaces/download/latest/?$								/shimmer/?download&appName=Hyperspaces&appVariant=Stable&appVersion=		            [L]
   - ^hyperspaces/download/([^/]+)/?$							/shimmer/?download&appName=Hyperspaces&appVariant=Stable&appVersion=$1	                [QSA,L]
   - ^hyperspaces/preview/appcast/?$								/shimmer/?appcast&appName=Hyperspaces&appVariant=Preview				                [QSA,L]
   - ^hyperspaces/preview/notes/?$ 								/shimmer/?notes&appName=Hyperspaces&appVariant=Preview									[QSA,L]
   - ^hyperspaces/preview/notes/([^/]+)/?$						/shimmer/?notes&appName=Hyperspaces&appVariant=Preview&appVersion=$1			        [QSA,L]
   - ^hyperspaces/preview/download/latest/?$						/shimmer/?download&appName=Hyperspaces&appVariant=Preview&appVersion=			        [L]
   - ^hyperspaces/preview/download/([^/]+)/?$					/shimmer/?download&appName=Hyperspaces&appVariant=Preview&appVersion=$1		            [QSA,L]
   - ^hyperspaces/private-beta/appcast/?$								/shimmer/?appcast&appName=Hyperspaces&appVariant=Private\ Beta				                [QSA,L]
   - ^hyperspaces/private-beta/notes/?$ 								/shimmer/?notes&appName=Hyperspaces&appVariant=Private\ Beta									[QSA,L]
   - ^hyperspaces/private-beta/notes/([^/]+)/?$						/shimmer/?notes&appName=Hyperspaces&appVariant=Private\ Beta&appVersion=$1			        [QSA,L]
   - ^hyperspaces/private-beta/download/latest/?$						/shimmer/?download&appName=Hyperspaces&appVariant=Private\ Beta&appVersion=			        [L]
   - ^hyperspaces/private-beta/download/([^/]+)/?$					/shimmer/?download&appName=Hyperspaces&appVariant=Private\ Beta&appVersion=$1		            [QSA,L]
   - ^hyperspaces/macheist3-stable/appcast/?$					/shimmer/?appcast&appName=Hyperspaces&appVariant=MacHeist3\ Stable						[QSA,L]
   - ^hyperspaces/macheist3-stable/notes/?$						/shimmer/?notes&appName=Hyperspaces&appVariant=MacHeist3\ Stable						[QSA,L]
   - ^hyperspaces/macheist3-stable/notes/([^/]+)/?$				/shimmer/?notes&appName=Hyperspaces&appVariant=MacHeist3\ Stable&appVersion=$1			[QSA,L]
   - ^hyperspaces/macheist3-stable/download/latest/?$			/shimmer/?download&appName=Hyperspaces&appVariant=MacHeist3\ Stable&appVersion=			[L]
   - ^hyperspaces/macheist3-stable/download/([^/]+)/?$			/shimmer/?download&appName=Hyperspaces&appVariant=MacHeist3\ Stable&appVersion=$1		[QSA,L]
   - ^hyperspaces/macheist3-preview/appcast/?$					/shimmer/?appcast&appName=Hyperspaces&appVariant=MacHeist3\ Preview						[QSA,L]
   - ^hyperspaces/macheist3-preview/notes/?$						/shimmer/?notes&appName=Hyperspaces&appVariant=MacHeist3\ Preview						[QSA,L]
   - ^hyperspaces/macheist3-preview/notes/([^/]+)/?$				/shimmer/?notes&appName=Hyperspaces&appVariant=MacHeist3\ Preview&appVersion=$1			[QSA,L]
   - ^hyperspaces/macheist3-preview/download/latest/?$			/shimmer/?download&appName=Hyperspaces&appVariant=MacHeist3\ Preview&appVersion=		[L]
   - ^hyperspaces/macheist3-preview/download/([^/]+)/?$		    /shimmer/?download&appName=Hyperspaces&appVariant=MacHeist3\ Preview&appVersion=$1	    [QSA,L]

document_root: /
index_list: [index.php]
default_gateway: index.php
php_version: 5.3.6
php_extensions: [eaccelerator, mysql, tidy, xml, xsl, zip]
php_date_timezone: Australia/Sydney
php_file_uploads: On
php_error_reporting: 'E_ALL'
php_display_errors: On
php_short_open_tag: On
php_default_mimetype: text/html
error_pages:
  "404": /404.html
  "403": /403.html