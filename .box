general: 
    writable_directories: [ /dir1 ]

php: 
    php_version: 5.2.7
    php_extensions: []
    php_error_reporting: E_ALL
    php_display_errors: On
    php_short_open_tag: On
    php_default_mimetype: text/html
    php_date_timezone: US/central
    php_max_execution_time: 30
    php_max_input_time: 60
    php_post_max_size: 8M
    php_file_uploads: On
    php_upload_max_filesize: 2M
    php_max_file_uploads: 20

web: 
    document_root: /
    index_list: [ index.html, fartfan222.php, index.php]
    far_future_expires: false
    permissions:
        /admin :
            - 127.0.0.1: allow
            - all: deny
        /test/phone-list.txt :
            - 123.5.15.8: deny
            - 123.5.15.9: deny
            - 123.5.15.10: deny
            - 123.5.15.11: deny
            - 123.5.15.12: deny
            - 123.5.15.13: deny
            - 123.5.15.14: deny
            - all: allow
    error_pages:
        "404": /404.html
        "403": /403.html